    "fbemitter": "^2.1.1",
    "i18next": "19.4.2",
    "i18next-browser-languagedetector": "^4.3.0",
    "lodash": "^4.17.15",
    "mockdate": "^2.0.5",
    "react": "16.11.0",
    "react-art": "16.11.0",
